    <method name="beforeFactory"><![CDATA[importPackage(Packages.org.eclipse.birt.report.model.api);
importPackage(Packages.org.eclipse.birt.report.model.api.elements);

var th = reportContext.getDesignHandle().findElement("mytable");
if (th) {
	var ph = th.getPropertyHandle(TableHandle.FILTER_PROP);
	
	if (ph) {
		ph.getContents();
	
		var firstFilter = true;
		iterator = ph.iterator();
		while (iterator.hasNext()) {
			var filterConditionHandle = iterator.next();

			var expr = new Packages.java.lang.String(filterConditionHandle.getExpr());
			if (expr.startsWith("row[") &amp;&amp; expr.endsWith("]"))
				expr = stripQuotes(expr.substring(4, expr.length() - 1));
			else if (expr.startsWith("[") &amp;&amp; expr.endsWith("]"))
				expr = stripQuotes(expr.substring(1, expr.length() -1));
				
			var value1 = "";
			var value2 = "";
			
			var value1List = filterConditionHandle.getValue1ExpressionList().getListValue();
			if (value1List != null) {
			for (var i = 0; i < value1List.size(); i++) {
				value1 += stripQuotes(value1List.get(i).getExpression());
				if (i < value1List.size() - 1)
					value1 += ", ";
			}
			
			if (filterConditionHandle.getValue2() != null)
				value2 = " and " + stripQuotes(filterConditionHandle.getValue2());
			
			if (!firstFilter)
				vars["filterStr"] += "\r";
				
			vars["filterStr"] += expr + " " + filterConditionHandle.getOperator() + " " + value1 + value2;
			
			firstFilter = false;
			}
		}
	}
}

function stripQuotes(val) {
	val = new Packages.java.lang.String(val);
	if (val.startsWith("\"") &amp;&amp; val.endsWith("\""))
		return val.substring(1, val.length() - 1);
	else
		return val;
		
}
]]></method>
